        li
          = image_tag "content/vitaly_200_200_ffffff_all_50.jpg", alt: "", height: "200", width: "200"
          span.team-member-name Vitaly
    section#contact.clearfix
      .section-header
        p
          span
            strong Contact
            | Us
            img src="http://www.agoragames.com/assets/images/icon-down.png"
      form#contact-form.simple action="http://www.agoragames.com/" method="post" name="a"
        .hiddenFields
          input name="ACT" type="hidden" value="26"
          input name="URI" type="hidden" value="us"
          input name="XID" type="hidden" value="26d01774cd9ffc898d344a1c2f7d97c7bef95cfb"
          input name="status" type="hidden" value="open"
          input name="return" type="hidden" value="thank-you"
          input name="redirect_on_duplicate" type="hidden" value=""
          input name="RET" type="hidden" value="http://www.agoragames.com/us"
          input name="form_name" type="hidden" value="Contact"
          input name="params_id" type="hidden" value="34760"
          input name="site_id" type="hidden" value="1"
        .block
          .text
            input#input-name name="name" placeholder="Name" type="text"
          .text
            input#input-email name="email" placeholder="Email" type="email"
          .text
            input#input-company name="company" placeholder="Company" type="text"
          .text
            input#input-phone name="phone" placeholder="Phone" type="text"
        .block-last
          .textarea
            textarea name="message"
        .submit.right
          input type="submit" value="Submit"
