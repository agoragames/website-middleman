      h1
        strong Team
      .callout
        a.laurel.scroll href="#contact"
    section.clearfix
      .section-header
        p
          span
            | The
            strong Team
            = image_tag 'icon-down.png', alt: ''
      ul.headshot-list
        li
          = image_tag "content/aaron_200_200_ffffff_all_50.jpg", alt: "", height: "200", width: "200"
          span.team-member-name Aaron
        li
          = image_tag "content/andrew2_200_200_ffffff_all_50.jpg", alt: "", height: "200", width: "200"
          span.team-member-name Andrew
        li
          = image_tag "content/Brad_200_190_ffffff_all_50.jpg", alt: "", height: "190", width: "200"
          span.team-member-name Brad
        li
          = image_tag "content/david_200_200_ffffff_all_50.jpg", alt: "", height: "200", width: "200"
          span.team-member-name David
        li
          = image_tag "content/dandrade_190_200_1_190_200_ffffff_all_50.png", alt: "", height: "200", width: "190"
          span.team-member-name David A
        li
          = image_tag "content/elliott_200_200_ffffff_all_50.jpg", alt: "", height: "200", width: "200"
          span.team-member-name Elliott
        li
          = image_tag "content/jack_200_200_ffffff_all_50.jpg", alt: "", height: "200", width: "200"
          span.team-member-name Jack
        li
          = image_tag "content/sam_200_200_ffffff_all_50.jpg", alt: "", height: "200", width: "200"
          span.team-member-name Sam
        li
          = image_tag "content/Steve-New-BW_190_200_ffffff_all_50.png", alt: "", height: "200", width: "190"
